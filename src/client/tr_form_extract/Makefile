$(MOC): 
	( cd $(QTDIR)/src/moc ; $(MAKE) )

Makefile: trfextract.pro  /usr/local/qt/mkspecs/default/qmake.conf /usr/local/qt/lib/libqt.prl
	$(QMAKE) -o Makefile trfextract.pro
