dist: 
	@mkdir -p .obj/trfextract && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .obj/trfextract/ && ( cd `dirname .obj/trfextract` && $(TAR) trfextract.tar trfextract && $(GZIP) trfextract.tar ) && $(MOVE) `dirname .obj/trfextract`/trfextract.tar.gz . && $(DEL_FILE) -r .obj/trfextract

